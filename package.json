{
  "name": "@pangolin/cli",
  "version": "1.8.1",
  "description": "Provides access to the global CLI interface for Pangolin",
  "author": {
    "name": "Fynn Becker",
    "email": "info@fynn-becker.de"
  },
  "homepage": "https://github.com/pangolinjs/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/pangolinjs/cli"
  },
  "bugs": {
    "url": "https://github.com/pangolinjs/cli/issues",
    "email": "info@fynn-becker.de"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "pretest": "npm run lint",
    "test": "npm-run-all -s test:clean test:create test:build test:build:dev",
    "test:clean": "rimraf ./test",
    "test:create": "node bin/pangolin.js create test",
    "test:build": "cd ./test && pangolin-core build",
    "test:build:dev": "cd ./test && pangolin-core build:dev"
  },
  "bin": {
    "pangolin": "./bin/pangolin.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.17.1",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.1.0"
  },
  "devDependencies": {
    "@pangolin/eslint-config": "^1.0.0",
    "eslint": "^5.3.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "license": "GPL-3.0"
}
